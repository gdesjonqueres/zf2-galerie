alias /blankapp /var/git/zf2/blank-app/public

<Directory /var/git/zf2/blank-app/public>
	Order deny,allow
	Allow from all

	AllowOverride None

	RewriteEngine On
	RewriteCond %{RESQUEST_FILENAME} -s [OR]
	RewriteCond %{RESQUEST_FILENAME} -l [OR]
	RewriteCond %{RESQUEST_FILENAME} -d
	RewriteBase /blankapp
	RewriteRule ^.*$ - [NC,L]
	RewriteRule !\.(js|ico|txt|gif|jpg|png|css)$ index.php [NC,L]
</Directory>
